# Wagtail API Configuration
# This should point to the Wagtail admin (used for session validation)
# The proxy will check /pages endpoint to validate the session
WAGTAIL_API_URL=https://api.sf.gov/admin

# Airtable Configuration
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here
AIRTABLE_TABLE_NAME=your_table_name_here

# Vercel KV Configuration (automatically provided by Vercel in production)
# KV_REST_API_URL=your_kv_rest_api_url
# KV_REST_API_TOKEN=your_kv_rest_api_token

# Cache Configuration (optional)
# SESSION_CACHE_TTL: Time-to-live for cached session validation results in seconds (default: 300 = 5 minutes)
# Acceptable range: 60-3600 (1 minute to 1 hour)
SESSION_CACHE_TTL=300

# WAGTAIL_VALIDATION_TIMEOUT: Timeout for Wagtail session validation requests in milliseconds (default: 5000 = 5 seconds)
# Acceptable range: 1000-10000 (1 second to 10 seconds)
WAGTAIL_VALIDATION_TIMEOUT=5000
