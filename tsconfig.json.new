{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"moduleDetection": "force",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noEmit": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"noUncheckedSideEffectImports": true
	},
	"files": []
}
